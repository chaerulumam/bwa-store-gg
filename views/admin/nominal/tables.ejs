<table id="example2" class="table table-bordered table-hover">
  <thead>
    <tr>
      <th>No</th>
      <th>Nama Coin</th>
      <th>Jumlah Coin</th>
      <th>Harga</th>
      <th>Aksi</th>
    </tr>
  </thead>
  <tbody>
    <% for( let i = 0; i < nominal.length; i++ ) { %>
    <tr>
      <td><%= i + 1 %></td>
      <td><%= nominal[i].coinName%></td>
      <td><%= nominal[i].coinQuantity%></td>
      <td><%= nominal[i].price%></td>
      <td>
        <form
          action="/nominal/delete/<%= nominal[i].id %>?_method=DELETE"
          method="POST"
        >
          <a
            href="/nominal/edit/<%= nominal[i]._id %> "
            class="btn btn-info btn-sm"
            ><i class="fas fa-pencil-alt"></i> Edit</a
          >

          <button type="submit" class="btn btn-danger btn-sm">
            <i class="fas fa-trash"> Delete</i>
          </button>
        </form>
      </td>
    </tr>
    <% } %>
  </tbody>
</table>
